<title>Test Page</title>

<!DOCTYPE html>
<html>
  <head>

  </head>
  <body>

    <div class="container mb-3">

        <h2 class="mb-3">파티룸 탐색</h2>
  
        <table border="1" class="board-table table table-sm border-bottom">
  
          <thead class="thead-light">
            <tr>
              <th scope="col">순번</th>
              <th scope="col">제목</th>
              <th scope="col">주제</th>
              <th scope="col">테마</th>
              <th scope="col">인원</th>
              <th scope="col">구분</th>
              <th scope="col">참여</th>
            </tr>
          </thead>
  
          <tbody>
            <% lists.forEach(function(list) { %>
            <tr>
              <td><%= list.room_id %></td>
              <td><%= list.room_title %></td>
              <td><%= list.room_subject %></td>
              <td><%= list.room_theme %></td>
              <td><%= list.room_private %></td>
              <td><%= list.room_pw %></td>
              <td><%= list.room_count %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
  </body>
</html>